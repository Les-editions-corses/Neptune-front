{% extends 'layout.html.twig' %}
{% block content %}
    {# Inclusions des zones #}
    {% for zone in page.zones %}
        {% if file_exist(templates_root~'/zone/'~zone.type.name~'.html.twig') %}
            {% include 'zone/'~(zone.type.name | lower )~'.html.twig' %}
        {% else %}
            {{ dump('ERROR : '~templates_root~'/zone/'~zone.type.name~'.html.twig  --> Nexiste pas ') }}
        {% endif %}
    {% endfor %}
    {# FIN d'inclusion des zones #}
{% endblock %}